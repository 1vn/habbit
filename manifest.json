{
  "manifest_version": 2,
  "name": "habbit",
  "short_name": "habbit",
  "description": "Remove addictiveness of reddit",
  "version": "0.01",
  "permissions": [
    "*://*.reddit.com/*",
    "storage"
  ],
  "content_scripts": [{
    "js": [ "jquery.min.js", "content.js" ],
    "matches": [ "*://*.reddit.com/*" ]    
  }]
}
